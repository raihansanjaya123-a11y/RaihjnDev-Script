local Rayfield = loadstring(game:HttpGet('https//sirius.menu/rayfield'))()

local Window = Rayfield:CrateWindow({
    Name = "üåçCraft A Worldüåç",
    LoadingTittle = "RaihjnDev",
    LoadingSubtittle = "By RaihjnDev",
    ConfigurationSaving = {
        Enable = false,
        FolderName = nil, -- Membuat kostume Folder buat diri sendiri hub/game
        FileName = "RaihjnScript"
    },
    Discrod = {
        Enable = true,
        Invite = "Raihjn", -- Discord invite code, jangan pakai discord.gg/. E.g. discord.gg/ABCD
        RememberJoins = true -- set ini jika tidak mau join discord setiap saat
    },
    KeySystem = true, -- set ini true untuk use key system
    KeySettings = {
        Tittle = "Craft A World | Key",
        Subtittle = "link In Discord Server",
        Note = "Join Server For Misc TAB",
        FileName = "RaihjnKey", -- di rekomendasikan menggunakan unik code
        SaveKey = true, -- gunakan ini jika ingin mengesave code 
        GrabKeyFormSite = true, -- set true jika kamu menggunakan web dan ambik pakai raw
        Key = {"https://pastebin.com/raw/TUwzzeDV"} -- masukkan raw link pw mu
    }
})

local MainTab = Window:CrateTab("üè¢HOME" , 448332458) -- Tittle, Image
local MainSection = MainTab:CrateSection("Section")

Rayfield:Notify({
    Title = "Raihjn Script",
    Content = "Script Berhasil Di Load",
    Duration = 5,
    Image = 448332458,
    Actions = {
        Ignore = {
            Name = "Okay!",
            Callback = function()
                print("Okay!")
            end
        },
    },
})

local Button = MainSection:CrateButton({
    Name = "Click Me",
    Callback = function()
        print("Button Clicked")
    end,
})

local Dropdown = MainSection:CrateDropdown({
    Name = "Select Block",
    Options = {"Option 1", "Option 2", "Option 3"},
    Callback = function(selected)
        print("Selected: " .. selected)
    end,
})

local dropdown2 = MainSection:CrateDropdown({
    Name = "Select Seed",
    Options = {"Option 1", "Option 2", "Option 3"},
    Callback = function(selected)
        print("Selected: " .. selected)
    end,
})
